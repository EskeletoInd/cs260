iv v-for=\"ticket in tickets\" v-bind:key=\"ticket.id\">\n    <hr>\n    <div class=\"ticket\">\n      <div class=\"problem\">\n        <p>{{ticket.problem}}</p>\n        <p><i>-- {{ticket.name}}</i></p>\n      </div>\n      <button @click=\"deleteTicket(ticket)\">Delete</button>\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'home',\n  data() {\n    return {\n      tickets: [],\n    }\n  },\n  created() {\n    this.getTickets();\n  },\n  methods: {\n    async getTickets() {\n      try {\n        let response = await axios.get(\"/api/tickets\");\n        this.tickets = response.data;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteTicket(ticket) {\n      try {\n        await axios.delete(\"/api/tickets/\" + ticket.id);\n        this.getTickets();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}